---
import FieldFormRow from './FieldFormRow.astro';

const currentLang = 'es';

interface Props {
  collection:any,
  field:any,
  parentField:any,
  document?: any,
  fieldCheckedDocsIds?:any,
  itemIndex?:any,
  parentName?:any,
  name?:any
  stage?:any,
}

const {collection, field, parentField, document, fieldCheckedDocsIds, itemIndex,name, parentName, stage }:any = Astro.props;

---
<FieldFormRow parentField={parentField} parentName={parentName} name={name} collection={collection} field={field} itemIndex={itemIndex}, document={stage}></FieldFormRow>
