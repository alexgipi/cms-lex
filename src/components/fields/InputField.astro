---
interface Props {
    name: string | null;
    id?: string | null;
    type: string | null;
    value?: string | null;
    placeholder?: string | null;
}

const { name, id, value, placeholder } = Astro.props;
const type:any= Astro.props.type;
---

<input 
type={type || 'text'} 
id={id} 
name={name} 
value={value} 
/>